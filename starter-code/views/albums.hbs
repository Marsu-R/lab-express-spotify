<div class="results-container">
    {{#each albumsList}}
    <div class="row">
        <div>
            <img class="image" src=" {{{ this.images.0.url }}} " alt="image of {{{ this.name }}}">
        </div>
        <div class="info-container">
            <p>{{{this.name}}}</p>
            <a href="/tracks/{{{this.id}}}"><button class="btn">View Tracks</button></a>
        </div>
    </div>
    {{/each}}
</div>